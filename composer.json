{
  "name"        : "backstagetechnicalservices/website",
  "description" : "The source code of the Backstage Technical Services website.",
  "keywords"    : [],
  "license"     : "GNU GPLv2",
  "type"        : "project",
  "require"     : {
    "php": "^8.2.0",
    "alaouy/youtube": "^2.0",
    "anhskohbo/no-captcha": "^3.0",
    "bugsnag/bugsnag-laravel": "^2.0",
    "components/jquery": "^3.1",
    "doctrine/dbal": "^3.0",
    "eluceo/ical": "^0.15",
    "eonasdan/bootstrap-datetimepicker": "^4.17",
    "fortawesome/font-awesome": "^4.7",
    "graham-campbell/markdown": "^15.2",
    "guzzlehttp/guzzle": "^7.2",
    "intervention/image": "^2.3",
    "konekt/html": "^6.5",
    "laminas/laminas-stdlib": "^3.0",
    "laravel/framework": "^11.0",
    "laravel/tinker": "^2.7",
    "laravel/ui": "^4.0",
    "lavary/laravel-menu": "^1.7",
    "mews/purifier": "^3.0",
    "moment/moment": "^2.17",
    "nunomaduro/collision": "^8.1",
    "select2/select2": "^4.0",
    "spatie/laravel-backup": "^8.6",
    "spatie/laravel-db-snapshots": "^2.6",
    "spatie/laravel-ignition": "^2.0",
    "twbs/bootstrap": "^3.3"
  },
  "require-dev" : {
    "barryvdh/laravel-debugbar": "^3.0",
    "fakerphp/faker": "^1.9.1",
    "filp/whoops": "^2.0",
    "mockery/mockery": "^1.4.2",
    "phpunit/php-code-coverage": "^10.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload"    : {
    "psr-4": {
      "App\\"                : "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\"  : "database/seeders/",
      "Package\\"            : "packages/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts"     : {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd"  : [
      "php artisan key:generate"
    ],
    "post-install-cmd"         : [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd"          : [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ],
    "post-autoload-dump"       : [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config"      : {
    "optimize-autoloader": true,
    "preferred-install"  : "dist",
    "sort-packages"      : true,
    "allow-plugins": {
      "composer/installers": true,
      "oomphinc/composer-installers-extender": true,
      "robloach/component-installer": true
    }
  },
  "repositories": [
  ]
}
