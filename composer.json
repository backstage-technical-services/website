{
  "name"        : "backstagetechnicalservices/website",
  "description" : "The source code of the Backstage Technical Services website.",
  "keywords"    : [],
  "license"     : "GNU GPLv2",
  "type"        : "project",
  "require"     : {
    "php"                               : "^8.0",
    "alaouy/youtube"                    : "^2.0",
    "anhskohbo/no-captcha"              : "^3.0",
    "bugsnag/bugsnag-laravel"           : "^2.0",
    "components/jquery"                 : "^3.1",
    "doctrine/dbal"                     : "^3.0",
    "eluceo/ical"                       : "^0.15",
    "eonasdan/bootstrap-datetimepicker" : "^4.17",
    "facade/ignition"                   : "^2.3.6",
    "fideloper/proxy"                   : "^4.0",
    "fortawesome/font-awesome"          : "^4.7",
    "fruitcake/laravel-cors"            : "^2.0",
    "graham-campbell/markdown"          : "^13.0",
    "guzzlehttp/guzzle"                 : "^7.0.1",
    "intervention/image"                : "^2.3",
    "laravel/framework"                 : "^8.0",
    "laravel/slack-notification-channel": "^2.0",
    "laravel/tinker"                    : "^2.0",
    "laravel/ui"                        : "^3.0",
    "laravelcollective/html"            : "^6.0",
    "lavary/laravel-menu"               : "^1.7",
    "magyarjeti/laravel-lipsum"         : "^1.1",
    "mews/purifier"                     : "^3.0",
    "moment/moment"                     : "^2.17",
    "select2/select2"                   : "^4.0",
    "spatie/laravel-backup"             : "^7.0",
    "spatie/laravel-db-snapshots"       : "^1.1",
    "twbs/bootstrap"                    : "^3.3",
    "laminas/laminas-stdlib"            : "^3.0"
  },
  "require-dev" : {
    "fakerphp/faker"           : "^1.9.1",
    "mockery/mockery"          : "^1.4.2",
    "phpunit/phpunit"          : "^9.5",
    "filp/whoops"              : "^2.0",
    "barryvdh/laravel-debugbar": "^3.0"
  },
  "autoload"    : {
    "psr-4": {
      "App\\"                : "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\"  : "database/seeders/",
      "Package\\"            : "packages/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts"     : {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd"  : [
      "php artisan key:generate"
    ],
    "post-install-cmd"         : [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd"          : [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ],
    "post-autoload-dump"       : [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config"      : {
    "optimize-autoloader": true,
    "preferred-install"  : "dist",
    "sort-packages"      : true
  },
  "repositories": [
  ]
}
